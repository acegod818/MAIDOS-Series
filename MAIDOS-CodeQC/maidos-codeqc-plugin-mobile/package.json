{
  "name": "@maidos/codeqc-plugin-mobile",
  "version": "0.1.0",
  "description": "Mobile languages support for MAIDOS CodeQC (Swift, Dart, Objective-C)",
  "type": "module",
  "main": "./dist/index.cjs",
