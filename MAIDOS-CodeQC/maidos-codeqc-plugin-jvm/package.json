{
  "name": "@maidos/codeqc-plugin-jvm",
  "version": "0.1.0",
  "description": "JVM languages support for MAIDOS CodeQC (Java, Kotlin, Scala, Groovy, Clojure)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "maidos",
    "codeqc",
    "plugin",
    "java",
    "kotlin",
    "scala",
    "groovy",
    "clojure",
    "jvm"
  ],
  "author": "MAIDOS",
  "license": "MIT",
  "peerDependencies": {
    "@maidos/codeqc": ">=0.1.0"
  },
  "devDependencies": {
    "@maidos/codeqc": "workspace:*",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
