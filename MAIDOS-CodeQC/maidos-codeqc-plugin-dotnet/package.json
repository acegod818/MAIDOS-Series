{
  "name": "@maidos/codeqc-plugin-dotnet",
  "version": "0.1.0",
  "description": ".NET languages support for MAIDOS CodeQC (C#, F#, VB.NET)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["maidos", "codeqc", "plugin", "csharp", "fsharp", "vbnet", "dotnet"],
  "author": "MAIDOS",
  "license": "MIT",
  "peerDependencies": {
    "@maidos/codeqc": ">=0.1.0"
  },
  "devDependencies": {
    "@maidos/codeqc": "workspace:*",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
